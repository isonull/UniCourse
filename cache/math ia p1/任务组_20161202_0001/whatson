<!-- http://webservices.internal.admin.cam.ac.uk/events/api/programmes/11/categories/show.xml --><!-- http://webservices.internal.admin.cam.ac.uk/events/api/programmes/11/calendar/from/20161202/to/20161202/show.xml?dateFormat=xsd&timeFormat=12hr&resultSetSize=20000 --><script type="text/javascript">
   $(document).ready(function() {
       // fudge layout
       var sidebarHeight = $("#events-sidebar").innerHeight();
       if ($("#content").height()<sidebarHeight) {
	 $("#content").height(sidebarHeight);
       }

       if ($("tr.past").size()) {
     if ($("tr.today, tr.future").size()) {
       $("tr.past").hide();
       $('#search-status').append('<p>Selection includes past events - <a href="#">Show</a></p>');

       $('#search-status a').toggle(
                    function() {
                      $(this).text('Hide');
                      $('tr.past').fadeIn('fast');
                    },
                    function() {
                      $(this).text('Show');
                      $('tr.past').fadeOut('fast');
                    } );
     } else {
       $('#search-status').append('<p>All the times shown are in the past. ' +
                      'View a specific event to see if it has more sessions.</p>');
     }
       }
       $(".ui-tabs-nav li").hover(function() { $(this).addClass('ui-state-hover'); },
                  function() { $(this).removeClass('ui-state-hover'); });

       $("input:submit, .control a").button();

     });
</script>

