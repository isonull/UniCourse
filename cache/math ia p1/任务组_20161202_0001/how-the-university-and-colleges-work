                <div class="cam-column9">
          <div class="cam-content-container">
            <h1 class="cam-sub-title">
              How the University and Colleges work            </h1>
          </div>
        </div>
      </div>
      </div>
  <!-- HORIZONTAL MENU + TOPIC TITLE -->
  <!-- CAROUSEL -->
    <!-- CAROUSEL -->
  <!-- CONTENT + NEWS + LEFT SIDEBAR + RIGHT SIDEBAR -->
  <div id="main" class="cam-row cam-content cam-recessed-content">
    <div class="cam-wrap clearfix">
                        <div class='cam-column3'>
            <!stopindex>
              <section class="region region-sidebar-first column sidebar">
    <div id="block-menu-block-10" class="block block-menu-block first last odd" role="navigation">
        <div class="menu-block-wrapper menu-block-10 menu-name-menu-about-university parent-mlid-0 menu-level-1">
<div class="cam-tertiary-navigation">
  <div class="cam-tertiary-navigation-structure">
    <ul class="cam-unstyled-list cam-vertical-breadcrumb">
    <li><a href="http://www.cam.ac.uk/about-the-university">About the University<span class="cam-vertical-breadcrumb-indicator"></span></a></li>
          </ul>
    
    <ul class="cam-unstyled-list cam-vertical-breadcrumb-navigation">
      <li class="menu__item is-active is-active-trail is-expanded first expanded active-trail active menu-mlid-836" ><a href="/about-the-university/how-the-university-and-colleges-work" class="menu__link is-active-trail active-trail active">How the University and Colleges work</a><ul class="cam-unstyled-list cam-vertical-breadcrumb-children local-dropdown-menu"><li class="menu__item is-leaf first leaf menu-mlid-1442" ><a href="/about-the-university/how-the-university-and-colleges-work/structure" class="menu__link">Structure</a></li>
