package tick2star;

import java.util.Stack;

public class LCSTopDownNonRecursive extends LCSFinder {

	public LCSTopDownNonRecursive(String s1, String s2) {
		super(s1, s2);
		if (mString1 == "" || mString2 == "") {
			mTable = null;
			return;
		}
		mTable = new int[s1.length()][s2.length()];
		for (int[] is : mTable) {
			for (int i = 0; i < s2.length(); ++i) {
				is[i] = -1;
			}
		}
	}

	private class pair {
		public final int i;
		public final int j;

		public pair(int x, int y) {
			i = x;
			j = y;
		}
	}

	@Override
	public int getLCSLength() {
		Stack<pair> stk = new Stack<>();
		stk.push(new pair(mString1.length()-1,mString1.length()-1));
		
		
		return 0;
	}

	@Override
	public String getLCSString() {
		// TODO Auto-generated method stub
		return null;
	}

}
