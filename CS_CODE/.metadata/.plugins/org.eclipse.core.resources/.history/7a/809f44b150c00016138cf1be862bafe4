package tick5;

import javax.swing.JFrame;

public class GUILife extends JFrame {
    public GUILife() {
        super("Game of Life");
        setDefaultCloseOperation(EXIT_ON_CLOSE);
        setSize(500,400);
    }

    public static void main(String[] args) {
        GUILife gui = new GUILife();
        gui.setVisible(true);

    }
}
