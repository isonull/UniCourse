package tick2star;

public class LCSTopDownRecursive extends LCSFinder {
	private String mLCS = "";

	public LCSTopDownRecursive(String s1, String s2) {
		super(s1, s2);
		if (mString1 == "" || mString2 == "") {
			mTable = null;
			return;
		}
		mTable = new int[s1.length()][s2.length()];
		for (int[] is : mTable) {
			for (int i = 0; i < s2.length(); ++i) {
				is[i] = -1;

			}
		}
	}

	@Override
	public int getLCSLength() {
		return 0;
	}

	@Override
	public String getLCSString() {
		return null;
	}

	@SuppressWarnings("unused")
	public static void main(String args[]) {
		LCSTopDownRecursive ins = new LCSTopDownRecursive("aferafg", "gfdsgfv");
		ins = new LCSTopDownRecursive("", "");

	}
}
